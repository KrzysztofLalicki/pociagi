                        <TableColumn fx:id="nr_wagonuCol2" prefWidth="75.0" text="nr. miejsca" />
                        <TableColumn fx:id="nr_miejscaCol2" prefWidth="75.0" text="nr. wagonu" />
                        <TableColumn fx:id="nr_przedzialuCol2" prefWidth="84.800048828125" text="nr. przedziału" />
                     </columns>
                  </TableView>
               </children>
            </VBox>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button minHeight="40.0" minWidth="70.0" mnemonicParsing="false" text="Dalej" onAction="#HandleButton"/>
         </children>
      </HBox>
   </children>
</VBox>
