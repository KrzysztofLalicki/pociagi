                  <Label text="Miejsca klasa I :" />
                  <TableView fx:id="tableView" maxHeight="600.0" maxWidth="258.0" minHeight="400.0" prefHeight="400.0" prefWidth="258.0">
                     <columns>
                        <TableColumn fx:id="nr_wagonuCol" prefWidth="75.0" text="nr. miejsca" />
                        <TableColumn fx:id="nr_miejscaCol" prefWidth="75.0" text="nr. wagonu" />
                        <TableColumn fx:id="nr_przedzialuCol" prefWidth="83.99993896484375" text="nr. przedziału" />
