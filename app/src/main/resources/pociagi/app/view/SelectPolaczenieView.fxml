<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<VBox fx:id="root" alignment="CENTER" maxWidth="-Infinity" spacing="10.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label alignment="TOP_LEFT" text="Wybierz połączenie" textAlignment="CENTER" VBox.vgrow="NEVER">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Label>
      <TableView VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
      </TableView>
      <Label text="Wybrane połączenie:" VBox.vgrow="NEVER">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Label>
      <HBox spacing="10.0" VBox.vgrow="NEVER">
         <children>
            <Label text="Stacja początkowa:" />
            <Label fx:id="stacjaPoczatkowa">
               <font>
                  <Font name="System Italic" size="13.0" />
               </font>
            </Label>
         </children>
      </HBox>
       <HBox spacing="10.0" VBox.vgrow="NEVER">
           <children>
               <Label text="Stacja końcowa:" />
               <Label fx:id="stacjaKoncowa">
                   <font>
                       <Font name="System Italic" size="13.0" />
                   </font>
               </Label>
           </children>
       </HBox>      <HBox spacing="10.0" VBox.vgrow="NEVER">
       <children>
           <Label text="Data odjazdu:" />
           <Label fx:id="dataOdjazdu">
               <font>
                   <Font name="System Italic" size="13.0" />
               </font>
           </Label>
       </children>
   </HBox>      <HBox spacing="10.0" VBox.vgrow="NEVER">
       <children>
           <Label text="Godzina odjazdu:" />
           <Label fx:id="godzinaOdjazdu">
               <font>
                   <Font name="System Italic" size="13.0" />
               </font>
           </Label>
       </children>
   </HBox>
   <HBox spacing="10.0" VBox.vgrow="NEVER">
    <children>
        <Label text="Czas podróży:" />
        <Label fx:id="czasPodrozy">
            <font>
                <Font name="System Italic" size="13.0" />
            </font>
        </Label>
    </children>
    </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
