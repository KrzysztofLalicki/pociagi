<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" prefHeight="573.0" prefWidth="537.0" spacing="10.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pociagi.app.viewmodel.TicketFindingConnectionViewModel">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <children>

        <HBox alignment="CENTER" prefHeight="120.0" prefWidth="517.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1">
            <children>
                <VBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="517.0" spacing="20">
                    <padding>
                        <Insets bottom="20" left="20" right="20" top="20" />
                    </padding>
                    <children>
                        <HBox alignment="CENTER" maxHeight="100.0" prefHeight="100.0" prefWidth="560.0" spacing="20" VBox.vgrow="ALWAYS">
                            <children>
                                <VBox alignment="CENTER" prefHeight="337.0" prefWidth="143.0" spacing="10" HBox.hgrow="ALWAYS">
                                    <children>
                                        <Label style="-fx-font-weight: bold;" text="Liczba miejsc klasa 1 :" />
                                        <ComboBox fx:id="comboBoxPlacesOne" prefWidth="140.0" />
                                    </children>
                                </VBox>
                                <VBox alignment="CENTER" prefWidth="140.0" spacing="10" HBox.hgrow="ALWAYS">
                                    <children>
                                        <Label style="-fx-font-weight: bold;" text="Liczba miejsc klasa 2 :" />
                                        <ComboBox fx:id="comboBoxPlacesTwo" prefWidth="140.0" />
                                    </children>
                                </VBox>
                                <VBox alignment="CENTER" prefWidth="140.0" spacing="10" HBox.hgrow="ALWAYS">
                                    <children>
                                        <Label style="-fx-font-weight: bold;" text="Data:" />
                                        <DatePicker fx:id="datePicker" minWidth="110.0" prefWidth="140.0" />
                                    </children>
                                </VBox>
                                <VBox alignment="CENTER" prefWidth="140.0" spacing="10" HBox.hgrow="ALWAYS">
                                    <children>
                                        <Label style="-fx-font-weight: bold;" text="Godzina:" />
                                        <HBox alignment="CENTER" spacing="5">
                                            <children>
                                                <Spinner fx:id="hourSpinner" editable="true" minWidth="60.0" prefWidth="60.0" />
                                                <Label text=":" />
                                                <Spinner fx:id="minuteSpinner" editable="true" minWidth="60.0" prefWidth="60.0" />
                                            </children>
                                        </HBox>
                                    </children>
                                </VBox>
                            </children>
                        </HBox>
                    </children>
                </VBox>
            </children>
        </HBox>
        <HBox spacing="20.0" VBox.vgrow="ALWAYS">
            <children>
                <VBox spacing="10.0" HBox.hgrow="ALWAYS">
                    <children>
                        <Label text="Wybierz stacje początkową">
                            <font>
                                <Font name="System Bold" size="13.0" />
                            </font></Label>
                        <fx:include fx:id="startStationView" source="SelectStationView.fxml" VBox.vgrow="ALWAYS" />
                    </children>
                </VBox>
                <VBox spacing="10.0" HBox.hgrow="ALWAYS">
                    <children>
                        <Label text="Wybierz stacje końcową">
                            <font>
                                <Font name="System Bold" size="13.0" />
                            </font></Label>
                        <fx:include fx:id="endStationView" source="SelectStationView.fxml" VBox.vgrow="ALWAYS" />
                    </children>
                </VBox>
            </children>
        </HBox>
    <HBox alignment="CENTER" spacing="10.0" VBox.vgrow="NEVER">
        <children>
            <VBox alignment="CENTER_LEFT" prefHeight="62.0" prefWidth="300.0">
               <children>
                  <Button mnemonicParsing="false" text="Cofnij" onAction="#CofnijButton" />
               </children>
            </VBox>
            <VBox alignment="CENTER_RIGHT" prefHeight="26.0" prefWidth="300.0">
               <children>
                  <Button fx:id="dalejButton" mnemonicParsing="false" onAction="#HandleButton" text="Dalej" />
               </children>
            </VBox>
        </children>
    </HBox>
</children>
</VBox>
